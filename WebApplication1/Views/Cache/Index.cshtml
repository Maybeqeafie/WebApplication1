@model IEnumerable<WebApplication1.Models.People>

<H1>@ViewBag.Text</H1>
@using (Html.BeginForm("Index", "Cache", method: FormMethod.Get))
{
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reward)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Reward)
                </td>

            </tr>
        }



    </table>
    <input type="submit" value="Данные из кеша" class="btn btn-default" />
}